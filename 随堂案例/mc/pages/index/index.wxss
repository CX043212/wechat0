/**index.wxss**/
.container{
  position: absolute;
  left:0;
  top:0;
  bottom: 100rpx;
  width: 100%;
}

#map{
  width: 100%;
  height: 100%;
}

.tabBar{
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
}
.tabBar>view{
  text-align: center;
  line-height: 100rpx;
  width: 100%;
  height: 100rpx;
  color: #fff;
}
.tabBar>view:nth-child(1){
  background: #ff9700;
}
.tabBar>view:nth-child(2){
  background: #03bbd5;
}
