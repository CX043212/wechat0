.row{
  display: flex;
  height: 100rpx;
  align-items: center;
  padding: 0 18rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f3f3f3; 
  font-size: 30rpx;
  color: #ccc;
}

.row>text{
  width: 200rpx;
}
.public{
  width: 90%;
  margin-top:50rpx;
  background: #ff9700;
  border:0;
  color: #fff;
}

/* success */

.success{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 40rpx;
}
.success>text{
  margin-top: 30rpx;
}
.success>button{
  width: 90%;
  background: #ff9700;
  color: #fff;
  margin-top: 100rpx;
}